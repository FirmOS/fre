<?xml version="1.0" encoding="UTF-8"?>
<CONFIG>
  <ProjectOptions>
    <Version Value="9"/>
    <General>
      <Flags>
        <MainUnitHasCreateFormStatements Value="False"/>
      </Flags>
      <SessionStorage Value="InIDEConfig"/>
      <MainUnit Value="0"/>
      <Title Value="FirmOS Generic #Patcher"/>
      <UseAppBundle Value="False"/>
      <ResourceType Value="res"/>
    </General>
    <i18n>
      <EnableI18N LFM="False"/>
    </i18n>
    <VersionInfo>
      <StringTable ProductVersion=""/>
    </VersionInfo>
    <BuildModes Count="1">
      <Item1 Name="Default" Default="True"/>
    </BuildModes>
    <PublishOptions>
      <Version Value="2"/>
      <IncludeFileFilter Value="*.(pas|pp|inc|lfm|lpr|lrs|lpi|lpk|sh|xml)"/>
      <ExcludeFileFilter Value="*.(bak|ppu|o|so);*~;backup"/>
    </PublishOptions>
    <RunParams>
      <local>
        <FormatVersion Value="1"/>
        <CommandLineParams Value="-e OE --patch=genauto"/>
        <LaunchingApplication PathPlusParams="/opt/local/bin/xterm -T 'Lazarus Run Output' -e $(LazarusDir)/tools/runwait.sh $(TargetCmdLine)"/>
      </local>
    </RunParams>
    <Units Count="6">
      <Unit0>
        <Filename Value="fre_patcher.lpr"/>
        <IsPartOfProject Value="True"/>
      </Unit0>
      <Unit1>
        <Filename Value="../fre_apps/fre_db_bbb_mod.pas"/>
        <IsPartOfProject Value="True"/>
      </Unit1>
      <Unit2>
        <Filename Value="../fre_apps/fre_db_bbbapp.pas"/>
        <IsPartOfProject Value="True"/>
      </Unit2>
      <Unit3>
        <Filename Value="../fre_core/fre_binary_buffer.pas"/>
        <IsPartOfProject Value="True"/>
      </Unit3>
      <Unit4>
        <Filename Value="../fre_db/fre_db_persistance_common.pas"/>
        <IsPartOfProject Value="True"/>
      </Unit4>
      <Unit5>
        <Filename Value="../fos_corebox/fos_dbcorebox_machine.pas"/>
        <IsPartOfProject Value="True"/>
      </Unit5>
    </Units>
  </ProjectOptions>
  <CompilerOptions>
    <Version Value="11"/>
    <Target>
      <Filename Value="../../bin/fre_patcher"/>
    </Target>
    <SearchPaths>
      <IncludeFiles Value="../../core/fos_include;$(ProjOutDir)"/>
      <OtherUnitFiles Value="../../core/fre_aps;../../core/fre_core;../../core/fre_fcom;../../core/fre_blockcom;../../core/fre_db;../../core/fos_interfaces;../../core/fre_server;../../core/fre_apps;../../core/fre_external/synapse;../../firmbox;../../hal;../../artemes/webapp;../../monsys/webapp;../../citycom/captiveportal;../../citycom/webapp"/>
      <UnitOutputDirectory Value="lib/$(TargetCPU)-$(TargetOS)"/>
    </SearchPaths>
    <Conditionals Value="if TargetOS = 'darwin' then
   CustomOptions := '-WM10.6 -dFOS_LINK_STATIC';
if TargetOS = 'solaris' then
   if TargetCPU='i386' then
     CustomOptions := '-k-rpath=$ORIGIN/../lib/lib32rel/solaris';
   if TargetCPU='i86_64' then
     CustomOptions := '-k-rpath=$ORIGIN/../lib/lib64rel/solaris';
if TargetOS = 'linux' then
   if TargetCPU='arm' then
     CustomOptions := '-k-rpath=$ORIGIN/../lib/libarmhf32rel/linux';
   if TargetCPU='i386' then
     CustomOptions := '-k-rpath=$ORIGIN/../lib/lib32rel/linux';
   if TargetCPU='i86_64' then
     CustomOptions := '-k-rpath=$ORIGIN/../lib/lib64rel/linux';"/>
    <Parsing>
      <SyntaxOptions>
        <IncludeAssertionCode Value="True"/>
      </SyntaxOptions>
    </Parsing>
    <CodeGeneration>
      <TargetCPU Value="arm"/>
    </CodeGeneration>
  </CompilerOptions>
  <Debugging>
    <Exceptions Count="3">
      <Item1>
        <Name Value="EAbort"/>
        <Enabled Value="False"/>
      </Item1>
      <Item2>
        <Name Value="ECodetoolError"/>
      </Item2>
      <Item3>
        <Name Value="EFOpenError"/>
      </Item3>
    </Exceptions>
  </Debugging>
</CONFIG>
